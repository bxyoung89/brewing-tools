{
  "name": "brewing-tools",
  "version": "1.0.0",
  "description": "Collection of random brewing tools I'm making",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "node build/clean.js",
    "build-all": "npm run lint && npm run clean && npm run build && npm run build-storybook",
    "build": "webpack --progress --config ./build/webpack.config.js",
    "build-svgs": "webpack --progress --config ./build/webpack.config.svg.js",
    "dev": "webpack-dev-server --config ./build/webpack.config.dev.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist/storybook",
    "lint": "npm run lint-js && npm run lint-scss",
    "lint-js": "node ./node_modules/eslint/bin/eslint.js --ext js,vue --fix ./src",
    "lint-scss": " node ./node_modules/stylelint/bin/stylelint.js --fix ./src",
    "prepush": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bxyoung89/brewing-tools.git"
